<html>

    <head>
        <link rel="stylesheet" href="/css/styles.css">
        <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">

        <title><%= title %></title>
    </head>

    <body>
        <div class="container">
            <center>
                <h1>Get the average availabilities of room types Entire home/apt and Private room listed in a certain neighbourhood group.</h1>
                <hr>

                <table class="table">
                    <tr>
                        <th><h3>room_type</h3></th>
                        <th><h3>neighbourhood_group</h3></th>
                        <th><h3>avg_availability</h3></th>
                    </tr>
                    <% for(var i = 0; i < results.length; i++) { %>
                        <tr>
                            <td class="row"><%= results[i].room_type %></td>
                            <td><%= results[i].neighbourhood_group %></td>
                            <td><%= results[i].avg_availability %></td>
                        </tr>
                    <% } %>
                </table>
            </center>
        </div>
    </body>

</html>