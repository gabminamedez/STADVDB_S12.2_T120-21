<html>

    <head>
        <link rel="stylesheet" href="/css/styles.css">
        <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">

        <title><%= title %></title>
    </head>

    <body>
        <div class="container">
            <center>
                <h1>Get the minimum and maximum price of listings in a certain neighbourhood.</h1>
                <hr>

                <table class="table">
                    <tr>
                        <th><h3>neighbourhood</h3></th>
                        <th><h3>min_price</h3></th>
                        <th><h3>max_price</h3></th>
                    </tr>
                    <% for(var i = 0; i < results.length; i++) { %>
                        <tr>
                            <td class="row"><%= results[i].neighbourhood %></td>
                            <td><%= results[i].min_price %></td>
                            <td><%= results[i].max_price %></td>
                        </tr>
                    <% } %>
                </table>
            </center>
        </div>
    </body>

</html>